{
  "manifest_version": 2,
  "name": "TabMan",
  "version": "1.0",

  "description": "Plugin for TabMan to sync tabs across multiple machines.",

  "icons": {
  },

  "permissions": ["tabs", "nativeMessaging"],

  "browser_specific_settings": {
    "gecko": {
      "id": "tabman@example.com",
      "strict_min_version": "50.0"
    }
  },

  "background": {
      "scripts": ["background.js"],
      "persistent": true
  }

}

