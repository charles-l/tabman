{
  "manifest_version": 2,
  "name": "TabMan",
  "version": "1.0",

  "description": "Plugin for TabMan to sync tabs across multiple machines.",

  "icons": {
  },

  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },


  "background": {
      "scripts": ["background.js"],
      "persistent": true
  }
}
